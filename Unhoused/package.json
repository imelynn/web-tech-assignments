{
  "name": "unhoused",
  "version": "1.0.0",
  "description": "Unhoused is an application to assist the homeless",
  "main": "index.js",
  "scripts": {
    "lint": "npx eslint src test",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "npx prettier src test --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "start": "vite",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "prettier -c .",
    "fix": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/FETA-Group/Unhoused.git"
  },
  "author": "FETA Group",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FETA-Group/Unhoused/issues"
  },
  "homepage": "https://github.com/FETA-Group/Unhoused#readme",
  "devDependencies": {
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "2.7.1"
  },
  "dependencies": {
    "vite": "^3.1.8"
  }
}
